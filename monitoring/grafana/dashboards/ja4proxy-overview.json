{
  "id": null,
  "uid": "ja4proxy-overview",
  "title": "JA4 Proxy - Security Overview",
  "tags": [
    "ja4",
    "security",
    "proxy"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m"
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Security Overview",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(ja4_requests_total[5m])) * 60",
          "legendFormat": "Requests/min",
          "refId": "A"
        },
        {
          "expr": "sum(rate(ja4_blocked_requests_total[5m])) * 60",
          "legendFormat": "Blocked/min",
          "refId": "B"
        },
        {
          "expr": "sum(rate(ja4_requests_total{action=\"blocked\"}[5m])) / clamp_min(sum(rate(ja4_requests_total[5m])), 1e-10) * 100",
          "legendFormat": "Block %",
          "refId": "C"
        },
        {
          "expr": "ja4_active_connections",
          "legendFormat": "Active Conns",
          "refId": "D"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 1
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Blocked/min"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Block %"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "orange"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Request Rate (req/sec)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum(rate(ja4_requests_total[1m]))",
          "legendFormat": "Total Requests",
          "refId": "A"
        },
        {
          "expr": "sum(rate(ja4_requests_total{action=\"allowed\"}[1m]))",
          "legendFormat": "Allowed",
          "refId": "B"
        },
        {
          "expr": "sum(rate(ja4_requests_total{action=\"blocked\"}[1m]))",
          "legendFormat": "Blocked",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Blocked" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Allowed" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "id": 3,
      "title": "Block Rate (%)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum(rate(ja4_requests_total{action=\"blocked\"}[5m])) / clamp_min(sum(rate(ja4_requests_total[5m])), 1e-10) * 100",
          "legendFormat": "Block Percentage",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "id": 4,
      "title": "Security Events by Type",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by (event_type) (increase(ja4_security_events_total[1h]))",
          "legendFormat": "{{event_type}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "pieType": "donut",
        "displayLabels": [
          "name",
          "percent"
        ],
        "legend": {
          "showLegend": true,
          "placement": "right"
        }
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "rate_limit_exceeded" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "validation_error" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "connection_error" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } }
            ]
          }
        ]
      }
    },
    {
      "id": 5,
      "title": "Top Blocked Reasons",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 16,
        "x": 8,
        "y": 12
      },
      "targets": [
        {
          "expr": "topk(10, sum by (reason, attack_type) (increase(ja4_blocked_requests_total[1h])))",
          "legendFormat": "",
          "refId": "A",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "reason": "Block Reason",
              "attack_type": "Attack Type",
              "Value": "Count"
            }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Count",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Count" },
            "properties": [
              { "id": "custom.displayMode", "value": "gradient-gauge" },
              { "id": "color", "value": { "mode": "continuous-RdYlGr" } }
            ]
          }
        ]
      }
    },
    {
      "id": 6,
      "title": "Rate Limit Events",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "sum(rate(ja4_security_events_total{event_type=\"rate_limit_exceeded\"}[5m]))",
          "legendFormat": "Rate Limit Exceeded",
          "refId": "A"
        },
        {
          "expr": "sum(rate(ja4_security_events_total{event_type=\"rate_limit_error\"}[5m]))",
          "legendFormat": "Rate Limit Errors",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Rate Limit Exceeded" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Rate Limit Errors" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "id": 7,
      "title": "Blocked by Reason",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "targets": [
        {
          "expr": "sum by (reason) (rate(ja4_blocked_requests_total[5m]))",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15,
            "showPoints": "never",
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "rate_limit" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "blacklist" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Internal error" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "purple" } }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "id": 8,
      "title": "Active Connections",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 28
      },
      "targets": [
        {
          "expr": "ja4_active_connections",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "never"
          },
          "color": {
            "mode": "fixed",
            "fixedColor": "blue"
          }
        }
      }
    },
    {
      "id": 9,
      "title": "Request Latency (p95, p99)",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 28
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ja4_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(ja4_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "id": 10,
      "title": "TLS Handshake Errors",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 34
      },
      "targets": [
        {
          "expr": "sum by (error_type) (rate(ja4_tls_handshake_errors_total[5m]))",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "calcs": ["mean", "max"]
        }
      }
    }
  ],
  "annotations": {
    "list": [
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "ALERTS{alertname=~\"JA4Proxy.*\"}",
        "iconColor": "red",
        "name": "Security Alerts",
        "tagKeys": "severity"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      }
    ]
  }
}