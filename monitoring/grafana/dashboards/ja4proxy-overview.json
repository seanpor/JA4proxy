{
  "id": null,
  "uid": "ja4proxy-overview",
  "title": "JA4 Proxy - Security Overview",
  "tags": [
    "ja4",
    "security",
    "proxy"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m"
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Security Overview",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(ja4_requests_total[5m])) * 60",
          "legendFormat": "Requests/min",
          "refId": "A"
        },
        {
          "expr": "sum(rate(ja4_blocked_requests_total[5m])) * 60",
          "legendFormat": "Blocked/min",
          "refId": "B"
        },
        {
          "expr": "ja4_whitelist_size",
          "legendFormat": "Whitelist",
          "refId": "C"
        },
        {
          "expr": "ja4_blacklist_size",
          "legendFormat": "Blacklist",
          "refId": "D"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Blocked/min"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Request Rate (req/sec)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "rate(ja4_requests_total[1m])",
          "legendFormat": "Total Requests",
          "refId": "A"
        },
        {
          "expr": "rate(ja4_blocked_requests_total[1m])",
          "legendFormat": "Blocked Requests",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec"
        },
        {
          "format": "short"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "points": false,
      "bars": false,
      "stack": false,
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "max": true
      }
    },
    {
      "id": 3,
      "title": "Block Rate (%)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "(rate(ja4_blocked_requests_total[5m]) / rate(ja4_requests_total[5m])) * 100",
          "legendFormat": "Block Percentage",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percent",
          "label": "Block Rate",
          "min": 0,
          "max": 100
        },
        {
          "format": "short"
        }
      ],
      "lines": true,
      "fill": 3,
      "linewidth": 2,
      "points": false,
      "bars": false,
      "stack": false,
      "thresholds": [
        {
          "value": 10,
          "colorMode": "critical",
          "op": "gt",
          "fill": true,
          "line": true
        }
      ],
      "alert": {
        "name": "High Block Rate",
        "message": "Block rate exceeds 10%",
        "conditions": [
          {
            "evaluator": {
              "params": [
                10
              ],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": [
                "A",
                "5m",
                "now"
              ]
            },
            "reducer": {
              "params": [],
              "type": "avg"
            },
            "type": "query"
          }
        ]
      }
    },
    {
      "id": 4,
      "title": "Security Events by Tier",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by (tier) (increase(ja4_security_events_total[1h]))",
          "legendFormat": "{{tier}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "pieType": "donut",
        "displayLabels": [
          "name",
          "percent"
        ],
        "legend": {
          "show": true,
          "placement": "right"
        }
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "banned"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "block"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "suspicious"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "yellow"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 5,
      "title": "Top 10 Blocked Fingerprints",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 16,
        "x": 8,
        "y": 12
      },
      "targets": [
        {
          "expr": "topk(10, sum by (ja4_fingerprint) (increase(ja4_blocked_requests_total[1h])))",
          "legendFormat": "",
          "refId": "A",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {},
            "renameByName": {
              "ja4_fingerprint": "JA4 Fingerprint",
              "Value": "Block Count"
            }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Block Count",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Block Count"
            },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "gradient-gauge"
              },
              {
                "id": "color",
                "value": {
                  "mode": "continuous-RdYlGr"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 6,
      "title": "Rate Limit Violations",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "sum by (strategy) (rate(ja4_rate_limit_exceeded_total[5m]))",
          "legendFormat": "{{strategy}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "ops",
          "label": "Violations/sec"
        },
        {
          "format": "short"
        }
      ],
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "bars": false,
      "stack": true,
      "legend": {
        "show": true,
        "alignAsTable": true,
        "current": true,
        "max": true
      }
    },
    {
      "id": 7,
      "title": "Whitelist/Blacklist Hits",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "targets": [
        {
          "expr": "rate(ja4_whitelist_hits_total[5m])",
          "legendFormat": "Whitelist Hits",
          "refId": "A"
        },
        {
          "expr": "rate(ja4_blacklist_hits_total[5m])",
          "legendFormat": "Blacklist Hits",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "ops",
          "label": "Hits/sec"
        },
        {
          "format": "short"
        }
      ],
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "bars": false,
      "stack": false,
      "legend": {
        "show": true,
        "alignAsTable": true,
        "current": true,
        "avg": true
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Whitelist Hits"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Blacklist Hits"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 8,
      "title": "Active Connections",
      "type": "graph",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 28
      },
      "targets": [
        {
          "expr": "ja4_active_connections",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Connections"
        },
        {
          "format": "short"
        }
      ],
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "points": false,
      "bars": false,
      "stack": false
    },
    {
      "id": 9,
      "title": "Request Latency (p95, p99)",
      "type": "graph",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 28
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(ja4_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, rate(ja4_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "points": false,
      "bars": false,
      "stack": false,
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "max": true
      }
    },
    {
      "id": 10,
      "title": "Recent Security Events",
      "type": "logs",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 34
      },
      "targets": [
        {
          "expr": "{job=\"ja4proxy\"} |= \"SECURITY\" or \"BLOCK\" or \"BAN\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "dedupStrategy": "none"
      }
    }
  ],
  "annotations": {
    "list": [
      {
        "datasource": "Prometheus",
        "enable": true,
        "expr": "ALERTS{alertname=\"JA4ProxyMaliciousFingerprint\"}",
        "iconColor": "red",
        "name": "Security Alerts",
        "tagKeys": "severity,alert_type"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      },
      {
        "name": "interval",
        "type": "interval",
        "query": "5m,10m,30m,1h,6h,12h,1d",
        "current": {
          "text": "5m",
          "value": "5m"
        }
      }
    ]
  }
}